{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://your-backend-url.onrender.com",
    "REACT_APP_WHISPER_API_URL": "https://your-whisper-api-url.onrender.com"
  }
} 